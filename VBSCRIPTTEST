PageUpdate.vb
Imports Google.Apis.Sheets.v4
Imports Google.Apis.Sheets.v4.Data

Public Class PageUpdate
    Private service As SheetsService
    Private searchTag As String
    Private userLobValue As String
    Private userAssignedValue As String
    Private dateAllocatedValue As String
    Private dateReturnedValue As String
    Private conditionValue As String
    Private commentsValue As String
    Private Const SpreadsheetId As String = "1JOWFl9A1NovpzC-RH3TgqqJOlAV19U2dnq7WtQ9RXJ0"
    Private progressForm As ProgressForm

    ' Constructor with SheetsService and searchTag parameters
    Public Sub New(service As SheetsService, searchTag As String, userLobValue As String, userAssignedValue As String, dateAllocatedValue As String, dateReturnedValue As String, conditionValue As String, commentsValue As String)
        InitializeComponent()
        Me.service = service
        Me.searchTag = searchTag
        Me.userLobValue = userLobValue
        Me.userAssignedValue = userAssignedValue
        Me.dateAllocatedValue = dateAllocatedValue
        Me.dateReturnedValue = dateReturnedValue
        Me.conditionValue = conditionValue
        Me.commentsValue = commentsValue
        InitializeForm()
    End Sub

    Private Sub InitializeForm()
        ' Set the values of the labels in PageUpdate
        userlobPrevious.Text = userLobValue
        userassignedPrevious.Text = userAssignedValue
        dateallocatedPrevious.Text = dateAllocatedValue
        datereturnedPrevious.Text = dateReturnedValue
        conditionPrevious.Text = conditionValue
        commentsPrevious.Text = commentsValue
    End Sub

    Private Sub Updateuserassigned_Click(sender As Object, e As EventArgs) Handles updateuserassigned.Click
        Dim newUserAssigned As String = textuserupdate.Text
        Dim newLocation As String = textlocationupdate.Text
        Dim newStatus As String = textstatusupdate.Text
        Dim newCondition As String = textconditionupdate.Text
        Dim newDateAllocated As String = textdateallocatedupdate.Text
        Dim newComments As String = textcommentsupdate.Text
        UpdateUserDataAsync(searchTag, newUserAssigned, newLocation, newStatus, newCondition, newDateAllocated, newComments)
    End Sub



    Private Async Sub UpdateUserDataAsync(tag As String, newUserAssigned As String, newLocation As String, newStatus As String, newCondition As String, newDateAllocated As String, newComments As String)
        ' Initialize the progress form
        If progressForm Is Nothing OrElse progressForm.IsDisposed Then
            progressForm = New ProgressForm()
        End If

        progressForm.Show()
        Application.DoEvents() ' Ensure the form is displayed immediately

        Try
            Dim sheetName As String = GetSheetNameFromTag(tag)
            If String.IsNullOrEmpty(sheetName) Then Exit Sub

            ' Fetch the entire range in one go
            Dim range As String = $"{sheetName}!A2:Q"
            Dim request As SpreadsheetsResource.ValuesResource.GetRequest = service.Spreadsheets.Values.Get(SpreadsheetId, range)
            Dim response As ValueRange = Await request.ExecuteAsync()
            Dim values As IList(Of IList(Of Object)) = response.Values

            Dim rowIndexToUpdate As Integer = -1
            If values IsNot Nothing AndAlso values.Count > 0 Then
                For i As Integer = 0 To values.Count - 1
                    Dim row As IList(Of Object) = values(i)
                    If row.Count > 6 AndAlso row(6).ToString() = tag Then
                        rowIndexToUpdate = i + 2 ' Adjusting for 1-based index
                        Exit For
                    End If
                Next
            End If

            If rowIndexToUpdate < 0 Then
                MessageBox.Show("Tag not found.")
                Exit Sub
            End If

            ' Run UpdatePreviousUserDataAsync first and check for errors
            Dim previousUserDataUpdateSuccessful As Boolean = Await UpdatePreviousUserDataAsync(sheetName, rowIndexToUpdate, tag)

            ' If UpdatePreviousUserDataAsync encountered an error, stop execution
            If Not previousUserDataUpdateSuccessful Then
                MessageBox.Show("Failed to user details to previous records. The update was aborted. Pleaes add more columns in the database")
                Return ' Exit the method if there was an error
            End If

            ' Prepare data for updating current user details
            Dim updates As New List(Of ValueRange) From {
            New ValueRange() With {.Range = $"{sheetName}!L{rowIndexToUpdate}", .Values = New List(Of IList(Of Object)) From {New List(Of Object)() From {newUserAssigned}}},
            New ValueRange() With {.Range = $"{sheetName}!J{rowIndexToUpdate}", .Values = New List(Of IList(Of Object)) From {New List(Of Object)() From {newLocation}}},
            New ValueRange() With {.Range = $"{sheetName}!I{rowIndexToUpdate}", .Values = New List(Of IList(Of Object)) From {New List(Of Object)() From {newStatus}}},
            New ValueRange() With {.Range = $"{sheetName}!E{rowIndexToUpdate}", .Values = New List(Of IList(Of Object)) From {New List(Of Object)() From {newCondition}}},
            New ValueRange() With {.Range = $"{sheetName}!O{rowIndexToUpdate}", .Values = New List(Of IList(Of Object)) From {New List(Of Object)() From {newDateAllocated}}},
            New ValueRange() With {.Range = $"{sheetName}!Q{rowIndexToUpdate}", .Values = New List(Of IList(Of Object)) From {New List(Of Object)() From {newComments}}}
        }

            Dim batchUpdateRequest As New BatchUpdateValuesRequest() With {
            .ValueInputOption = "RAW",
            .Data = updates
        }

            Dim batchUpdate As SpreadsheetsResource.ValuesResource.BatchUpdateRequest = service.Spreadsheets.Values.BatchUpdate(batchUpdateRequest, SpreadsheetId)
            Await batchUpdate.ExecuteAsync()

            MessageBox.Show($"{tag} details have been updated.")
        Catch ex As Exception
            MessageBox.Show("An error occurred: " & ex.Message)
        Finally
            If progressForm IsNot Nothing AndAlso Not progressForm.IsDisposed Then
                progressForm.Close()
            End If
        End Try


    End Sub


    Private Async Function UpdateSheetValuesAsync(batchUpdateRequest As BatchUpdateValuesRequest) As Task
        Dim batchUpdate As SpreadsheetsResource.ValuesResource.BatchUpdateRequest = service.Spreadsheets.Values.BatchUpdate(batchUpdateRequest, SpreadsheetId)
        Await batchUpdate.ExecuteAsync()
    End Function

    Private Async Function UpdatePreviousUserDataAsync(sheetName As String, rowIndex As Integer, tag As String) As Task(Of Boolean)
        ' Initialize the progress form
        If progressForm Is Nothing OrElse progressForm.IsDisposed Then
            progressForm = New ProgressForm()
        End If

        progressForm.Show()
        Application.DoEvents() ' Ensure the form is displayed immediately

        Try
            ' Define the starting column and number of columns to check
            Dim startingColumn As String = "R" ' Starting column
            Dim columnOffset As Integer = 6 ' Number of columns per set of previous user data

            ' Prepare the values to be inserted
            Dim previousUserValues As New List(Of Object) From {
            userlobPrevious.Text,
            userassignedPrevious.Text,
            dateallocatedPrevious.Text,
            datereturnedPrevious.Text,
            conditionPrevious.Text,
            commentsPrevious.Text
        }

            ' Convert starting column to its numeric index (A=1, B=2, ..., R=18)
            Dim startingIndex As Integer = ColumnLetterToIndex(startingColumn)

            ' Check columns from the starting column
            For i As Integer = 0 To Integer.MaxValue ' Loop until we find available columns or reach the end
                Dim currentColumnIndex As Integer = startingIndex + (i * columnOffset)
                Dim columnRange As String = IndexToColumnLetter(currentColumnIndex) ' Convert index back to column letter

                ' Check if the columns exceed the maximum valid column letter
                If String.IsNullOrEmpty(columnRange) Then
                    MessageBox.Show("No available columns found for previous user data.")
                    Return False ' Return false to indicate failure
                End If

                ' Define the range for checking blank cells
                Dim rangeToCheck As String = $"{sheetName}!{columnRange}{rowIndex}:{IndexToColumnLetter(currentColumnIndex + columnOffset - 1)}{rowIndex}"
                Dim request As SpreadsheetsResource.ValuesResource.GetRequest = service.Spreadsheets.Values.Get(SpreadsheetId, rangeToCheck)
                Dim response As ValueRange = Await request.ExecuteAsync()
                Dim values As IList(Of IList(Of Object)) = response.Values

                ' Check if all columns in the range are empty
                Dim isEmpty As Boolean = True
                If values IsNot Nothing AndAlso values.Count > 0 Then
                    For Each value In values(0)
                        If Not String.IsNullOrEmpty(value?.ToString()) Then
                            isEmpty = False
                            Exit For
                        End If
                    Next
                End If

                If isEmpty Then
                    ' Update the blank cells with previous user data
                    Dim previousUserUpdates As New List(Of ValueRange) From {
                    New ValueRange() With {.Range = $"{sheetName}!{columnRange}{rowIndex}", .Values = New List(Of IList(Of Object)) From {New List(Of Object)() From {previousUserValues(0)}}},
                    New ValueRange() With {.Range = $"{sheetName}!{IndexToColumnLetter(currentColumnIndex + 1)}{rowIndex}", .Values = New List(Of IList(Of Object)) From {New List(Of Object)() From {previousUserValues(1)}}},
                    New ValueRange() With {.Range = $"{sheetName}!{IndexToColumnLetter(currentColumnIndex + 2)}{rowIndex}", .Values = New List(Of IList(Of Object)) From {New List(Of Object)() From {previousUserValues(2)}}},
                    New ValueRange() With {.Range = $"{sheetName}!{IndexToColumnLetter(currentColumnIndex + 3)}{rowIndex}", .Values = New List(Of IList(Of Object)) From {New List(Of Object)() From {previousUserValues(3)}}},
                    New ValueRange() With {.Range = $"{sheetName}!{IndexToColumnLetter(currentColumnIndex + 4)}{rowIndex}", .Values = New List(Of IList(Of Object)) From {New List(Of Object)() From {previousUserValues(4)}}},
                    New ValueRange() With {.Range = $"{sheetName}!{IndexToColumnLetter(currentColumnIndex + 5)}{rowIndex}", .Values = New List(Of IList(Of Object)) From {New List(Of Object)() From {previousUserValues(5)}}}
                }

                    Dim batchUpdateRequest As New BatchUpdateValuesRequest() With {
                    .ValueInputOption = "RAW",
                    .Data = previousUserUpdates
                }

                    Dim batchUpdate As SpreadsheetsResource.ValuesResource.BatchUpdateRequest = service.Spreadsheets.Values.BatchUpdate(batchUpdateRequest, SpreadsheetId)
                    Await batchUpdate.ExecuteAsync()

                    Return True ' Return true to indicate success
                End If
            Next

            MessageBox.Show("No available columns found for previous user data.")
            Return False ' Return false to indicate failure
        Catch ex As Exception
            MessageBox.Show("An error occurred while updating previous user data: " & ex.Message)
            Return False ' Return false to indicate failure
        Finally
            If progressForm IsNot Nothing AndAlso Not progressForm.IsDisposed Then
                progressForm.Close()
            End If
        End Try
    End Function



    Private Function ColumnLetterToIndex(columnLetter As String) As Integer
        Dim columnIndex As Integer = 0
        Dim length As Integer = columnLetter.Length

        For i As Integer = 0 To length - 1
            columnIndex = (columnIndex * 26) + (Asc(columnLetter(i)) - Asc("A") + 1)
        Next

        Return columnIndex
    End Function

    Private Function IndexToColumnLetter(index As Integer) As String
        Dim columnLetter As String = ""

        While index > 0
            Dim modValue As Integer = (index - 1) Mod 26
            columnLetter = Chr(Asc("A") + modValue) & columnLetter
            index = (index - modValue) \ 26
        End While

        Return columnLetter
    End Function



    Private Function GetSheetNameFromTag(tag As String) As String
        If tag.StartsWith("MS-LTP") Then
            Return "LAPTOP"
        ElseIf tag.StartsWith("MS-MNTR") Then
            Return "Monitor"
        ElseIf tag.StartsWith("MS-SU") Then
            Return "SystemUnit"
        Else
            MessageBox.Show("Invalid tag format.")
            Return String.Empty
        End If
    End Function

End Class

