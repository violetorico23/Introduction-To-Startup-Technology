LoginForm.vb
Imports System.IO
Imports System.Threading
Imports System.Windows.Forms
Imports Google.Apis.Auth.OAuth2
Imports Google.Apis.Services
Imports Google.Apis.Sheets.v4
Imports Google.Apis.Util.Store

Public Class LoginForm
    Public Property Service As SheetsService

    Private Async Sub btnLogin_Click(sender As Object, e As EventArgs) Handles LoginButton.Click
        Try
            Await InitializeGoogleSheetsAPIAsync()
            MessageBox.Show("Login successful!")
            Me.DialogResult = DialogResult.OK
            Me.Close()
        Catch ex As Exception
            MessageBox.Show("An error occurred during authentication: " & ex.Message)
        End Try
    End Sub

    Private Async Function InitializeGoogleSheetsAPIAsync() As Task
        Dim credPath As String = "token.json"
        Using stream = New FileStream("D:\TEST\VBSCRIPTTEST\client_secret_12727952522-6bmg328tk6cfkcqirgnitini759bs8pa.apps.googleusercontent.com.json", FileMode.Open, FileAccess.Read)
            Dim credential = Await GoogleWebAuthorizationBroker.AuthorizeAsync(
                GoogleClientSecrets.FromStream(stream).Secrets,
                New String() {SheetsService.Scope.Spreadsheets},
                "user",
                CancellationToken.None,
                New FileDataStore(credPath, True))

            Service = New SheetsService(New BaseClientService.Initializer() With {
                .HttpClientInitializer = credential,
                .ApplicationName = "VBSCRIPTTEST"
            })
        End Using
    End Function
End Class

