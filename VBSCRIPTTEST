MainPage.vb
Imports System
Imports System.Windows.Forms
Imports Google.Apis.Auth.OAuth2
Imports Google.Apis.Services
Imports Google.Apis.Sheets.v4
Imports Google.Apis.Sheets.v4.Data
Imports Google.Apis.Util.Store
Imports System.IO
Imports System.Threading
Imports System.Windows.Forms.VisualStyles.VisualStyleElement

Public Class MainPage
    Private Shared ReadOnly ApplicationName As String = "VBSCRIPTTEST"
    Private Shared ReadOnly SpreadsheetId As String = "1JOWFl9A1NovpzC-RH3TgqqJOlAV19U2dnq7WtQ9RXJ0"
    Private Const RangeBase As String = "A4:Q"
    Private Shared ReadOnly Scopes As String() = {SheetsService.Scope.Spreadsheets}
    Private service As SheetsService

    Private Sub MainPage_Load(sender As Object, e As EventArgs) Handles MyBase.Load
        ' Assign context menu to all labels
        For Each ctrl As Control In Me.Controls
            If TypeOf ctrl Is Label Then
                ctrl.ContextMenuStrip = contextMenuStrip1
            End If
        Next
        InitializeGoogleSheetsAPI()
        AddHandler searchTagTextBox.KeyDown, AddressOf SearchTagTextBox_KeyDown

        Dim LoginForm As New LoginForm()
        If loginForm.ShowDialog() = DialogResult.OK Then
            ' Authentication was successful
            service = LoginForm.Service
            ' Initialize main page functionality here
            InitializeUI()
        Else
            MessageBox.Show("Failed to authenticate with Google.")
            Application.Exit()
        End If
    End Sub

    Private Sub InitializeUI()
        ' Example: Set initial visibility of controls
        brand1.Visible = True
        assettag1.Visible = True
        serialnumber1.Visible = True
        userassigned1.Visible = True
        location1.Visible = True
        po1.Visible = True
        price1.Visible = True
        condition1.Visible = True
        hostname1.Visible = True
        modelnumber1.Visible = True
        equipmentlob1.Visible = True
        status1.Visible = True
        userlob1.Visible = True
        idnumber1.Visible = True
        dateallocated1.Visible = True
        datereturned1.Visible = True
        comments1.Visible = True

        ' Example: Clear text boxes or labels
        searchTagTextBox.Text = String.Empty
        brand1.Text = String.Empty
        assettag1.Text = String.Empty
        serialnumber1.Text = String.Empty
        userassigned1.Text = String.Empty
        location1.Text = String.Empty
        po1.Text = String.Empty
        price1.Text = String.Empty
        condition1.Text = String.Empty
        hostname1.Text = String.Empty
        modelnumber1.Text = String.Empty
        equipmentlob1.Text = String.Empty
        status1.Text = String.Empty
        userlob1.Text = String.Empty
        idnumber1.Text = String.Empty
        dateallocated1.Text = String.Empty
        datereturned1.Text = String.Empty
        comments1.Text = String.Empty

        ' Example: Set default values or states
        searchTagTextBox.Focus() ' Set focus to the search tag textbox
        hostnameHeader.Visible = False
        hostname1.Visible = False
        modelnumberHeader.Visible = True
        modelnumber1.Visible = True

        ' Example: Set up event handlers if not already done
        AddHandler SearchButton.Click, AddressOf SearchButton_Click
        AddHandler updatepage.Click, AddressOf Updatepage_Click

        ' Initialize any additional controls or settings
        ' e.g., Load default data, set initial states of combo boxes, etc.
    End Sub

    Private Sub CopyToolStripMenuItem_Click(sender As Object, e As EventArgs) Handles copyToolStripMenuItem.Click
        Dim menu As ToolStripMenuItem = CType(sender, ToolStripMenuItem)
        Dim contextMenu As ContextMenuStrip = CType(menu.GetCurrentParent(), ContextMenuStrip)
        Dim sourceControl As Control = contextMenu.SourceControl

        If TypeOf sourceControl Is Label Then
            Dim label As Label = CType(sourceControl, Label)
            Clipboard.SetText(label.Text)
        End If
    End Sub


    Private Sub InitializeGoogleSheetsAPI()
        Using stream = New FileStream("D:\TEST\VBSCRIPTTEST\client_secret_12727952522-6bmg328tk6cfkcqirgnitini759bs8pa.apps.googleusercontent.com.json", FileMode.Open, FileAccess.Read)
            Dim credPath As String = "token.json"
            Dim credential = GoogleWebAuthorizationBroker.AuthorizeAsync(
                GoogleClientSecrets.FromStream(stream).Secrets,
                Scopes,
                "user",
                CancellationToken.None,
                New FileDataStore(credPath, True)).Result

            service = New SheetsService(New BaseClientService.Initializer() With {
                .HttpClientInitializer = credential,
                .ApplicationName = ApplicationName
            })
        End Using
    End Sub

    Private Sub SearchButton_Click(sender As Object, e As EventArgs) Handles SearchButton.Click
        Dim searchTag As String = searchTagTextBox.Text

        ' Validate the search tag
        If String.IsNullOrWhiteSpace(searchTag) OrElse Not IsValidTagPrefix(searchTag) Then
            MessageBox.Show("Please enter a valid tag.")
            Return
        End If

        ' Proceed with data retrieval and display
        SearchAndDisplayData(searchTag)
    End Sub

    Private Function IsValidTagPrefix(tag As String) As Boolean
        Return tag.StartsWith("MS-LTP") OrElse tag.StartsWith("MS-SU")
    End Function

    Private Sub SearchAndDisplayData(searchTag As String)
        Try
            ' Proceed with data retrieval and display
            Dim sheetName As String = GetSheetNameFromTag(searchTag)
            Dim range As String = $"{sheetName}!A2:Z"
            Dim request As SpreadsheetsResource.ValuesResource.GetRequest = service.Spreadsheets.Values.Get(SpreadsheetId, range)
            Dim response As ValueRange = request.Execute()
            Dim values As IList(Of IList(Of Object)) = response.Values

            Dim dataFound As Boolean = False

            If values IsNot Nothing AndAlso values.Count > 0 Then
                For Each row As IList(Of Object) In values
                    If row.Count > 6 AndAlso row(6).ToString() = searchTag Then
                        brand1.Text = If(row.Count > 2, row(2).ToString(), String.Empty)
                        assettag1.Text = If(row.Count > 6, row(6).ToString(), String.Empty)
                        serialnumber1.Text = If(row.Count > 5, row(5).ToString(), String.Empty)
                        userassigned1.Text = If(row.Count > 11, row(11).ToString(), String.Empty)
                        location1.Text = If(row.Count > 9, row(9).ToString(), String.Empty)
                        po1.Text = If(row.Count > 0, row(0).ToString(), String.Empty)
                        price1.Text = If(row.Count > 1, row(1).ToString(), String.Empty)
                        condition1.Text = If(row.Count > 4, row(4).ToString(), String.Empty)
                        hostname1.Text = If(row.Count > 3, row(3).ToString(), String.Empty)
                        modelnumber1.Text = If(row.Count > 3, row(3).ToString(), String.Empty)
                        equipmentlob1.Text = If(row.Count > 10, row(10).ToString(), String.Empty)
                        status1.Text = If(row.Count > 8, row(8).ToString(), String.Empty)
                        userlob1.Text = If(row.Count > 13, row(13).ToString(), String.Empty)
                        idnumber1.Text = If(row.Count > 12, row(12).ToString(), String.Empty)
                        dateallocated1.Text = If(row.Count > 14, row(14).ToString(), String.Empty)
                        datereturned1.Text = If(row.Count > 15, row(15).ToString(), String.Empty)
                        comments1.Text = If(row.Count > 16, row(16).ToString(), String.Empty)
                        dataFound = True
                        Exit For
                    End If
                Next

                If Not dataFound Then
                    ' Display "Tag not found" message
                    brand1.Text = String.Empty
                    assettag1.Text = String.Empty
                    serialnumber1.Text = String.Empty
                    userassigned1.Text = String.Empty
                    location1.Text = String.Empty
                    po1.Text = String.Empty
                    price1.Text = String.Empty
                    condition1.Text = String.Empty
                    hostname1.Text = String.Empty
                    modelnumber1.Text = String.Empty
                    equipmentlob1.Text = String.Empty
                    status1.Text = String.Empty
                    userlob1.Text = String.Empty
                    idnumber1.Text = String.Empty
                    dateallocated1.Text = String.Empty
                    datereturned1.Text = String.Empty
                    comments1.Text = String.Empty

                    MessageBox.Show("Tag not found.")
                End If
            Else
                ' Handle case where no values are returned from the API
                brand1.Text = String.Empty
                assettag1.Text = String.Empty
                serialnumber1.Text = String.Empty
                userassigned1.Text = String.Empty
                location1.Text = String.Empty
                po1.Text = String.Empty
                price1.Text = String.Empty
                condition1.Text = String.Empty
                hostname1.Text = String.Empty
                modelnumber1.Text = String.Empty
                equipmentlob1.Text = String.Empty
                status1.Text = String.Empty
                userlob1.Text = String.Empty
                idnumber1.Text = String.Empty
                dateallocated1.Text = String.Empty
                datereturned1.Text = String.Empty
                comments1.Text = String.Empty

                MessageBox.Show("Tag not found.")
            End If
        Catch ex As Exception
            ' Handle invalid tag format or other exceptions
            brand1.Text = String.Empty
            assettag1.Text = String.Empty
            serialnumber1.Text = String.Empty
            userassigned1.Text = String.Empty
            location1.Text = String.Empty
            po1.Text = String.Empty
            price1.Text = String.Empty
            condition1.Text = String.Empty
            hostname1.Text = String.Empty
            modelnumber1.Text = String.Empty
            equipmentlob1.Text = String.Empty
            status1.Text = String.Empty
            userlob1.Text = String.Empty
            idnumber1.Text = String.Empty
            dateallocated1.Text = String.Empty
            datereturned1.Text = String.Empty
            comments1.Text = String.Empty

            MessageBox.Show("An error occurred while searching for the tag.")
        End Try

        ' Check if the tag prefix is valid and set visibility of hostname labels
        If IsValidTagPrefix(searchTag) Then
            hostnameHeader.Visible = True
            hostname1.Visible = True
            modelnumberHeader.Visible = False
            modelnumber1.Visible = False
        Else
            hostnameHeader.Visible = False
            hostname1.Visible = False
            modelnumberHeader.Visible = True
            modelnumber1.Visible = True
        End If
    End Sub

    Private Function GetSheetNameFromTag(tag As String) As String
        If tag.StartsWith("MS-LTP") Then
            Return "LAPTOP"
        ElseIf tag.StartsWith("MS-MNTR") Then
            Return "Monitor"
        ElseIf tag.StartsWith("MS-SU") Then
            Return "SystemUnit"
        Else
            MessageBox.Show("Invalid tag format.")
            Return String.Empty
        End If
    End Function

    Private Sub SearchTagTextBox_KeyDown(sender As Object, e As KeyEventArgs)
        If e.KeyCode = Keys.Enter Then
            Dim searchTag As String = searchTagTextBox.Text

            ' Validate the search tag
            If String.IsNullOrWhiteSpace(searchTag) OrElse Not IsValidTagPrefix(searchTag) Then
                MessageBox.Show("Please enter a valid tag.")
                Return
            End If

            ' Proceed with data retrieval and display
            SearchAndDisplayData(searchTag)
        End If
    End Sub


    Private Sub Updatepage_Click(sender As Object, e As EventArgs) Handles updatepage.Click
        Dim searchTag As String = searchTagTextBox.Text
        Dim userLobValue As String = userlob1.Text
        Dim userAssignedValue As String = userassigned1.Text
        Dim dateAllocatedValue As String = dateallocated1.Text
        Dim dateReturnedValue As String = datereturned1.Text
        Dim conditionValue As String = condition1.Text
        Dim commentsValue As String = comments1.Text

        ' Validate the search tag
        If String.IsNullOrWhiteSpace(searchTag) OrElse Not IsValidTagPrefix(searchTag) Then
            MessageBox.Show("Please enter a valid tag.")
            Return
        End If

        Dim pageUpdate As New PageUpdate(service, searchTag, userLobValue, userAssignedValue, dateAllocatedValue, dateReturnedValue, conditionValue, commentsValue)
        pageUpdate.ShowDialog()
    End Sub

End Class


